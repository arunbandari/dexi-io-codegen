var DexiBaseModel = require('../DexiBaseModel');

/**
 * {{model.description}}
 **/
function {{model.className}}() {

    {{#each model.fields}}
    {{#if this.description}}
    /**
     * {{this.description}}
     * @type {{curly this.className}} {{#if enum}}( One of {{stringList enum}} ){{/if}}
     **/
    {{/if}}
